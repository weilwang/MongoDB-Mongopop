    "external-ip": "^0.2.4",
    "jade": "~1.11.0",
    "methods": "^1.1.2",
    "mongodb": "^2.2.5",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "request": "^2.74.0",
    "serve-favicon": "^2.5.0",
    "tsc": "^1.20150623.0"
  }
}
